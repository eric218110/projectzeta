{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web",
      "type": "dart",
      "request": "launch",
      "program": "lib/main/main.dart",
      "args": ["-d", "chrome"],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter iOS",
      "type": "dart",
      "request": "launch",
      "program": "lib/main/main.dart",
      "deviceId": "ios",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter Android",
      "type": "dart",
      "request": "launch",
      "program": "lib/main/main.dart",
      "deviceId": "android",
      "cwd": "${workspaceFolder}"
    }
  ]
}
